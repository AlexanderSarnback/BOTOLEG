{
  "type": "AdaptiveCard",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "${file}"
    },
    {
      "type": "TextBlock",
      "text": "${filebody}",
      "wrap": true
    }
  ],
  "actions": [
    {
      "type": "Action.OpenUrl",
      "title": "File SCAN: Virus Total",
      "url": "https://www.virustotal.com/gui/home/upload"
    },
    {
      "type": "Action.OpenUrl",
      "title": "File SCAN: Open Tip",
      "url": "https://opentip.kaspersky.com/"
    }
  ],
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "version": "1.4"
}
